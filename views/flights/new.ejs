<%- include('../partials/header') %>
<form class="create-form" action="/flights" method="POST">
  <div class="wrapper">
    <img src="https://cdn-icons-png.flaticon.com/512/4353/4353032.png" alt="" class="img-icon">
    <label>Airline</label>
  <select name="airline">
    <option value="Southwest">Southwest</option>
    <option value="American">American</option>
    <option value="United">United</option>
  </select>

  <label>Airport</label>
  <select name="airport">
    <option value="AUS">AUS</option>
    <option value="DFW">DFW</option>
    <option value="DEN" selected>DEN</option>
    <option value="LAX">LAX</option>
    <option value="SAN">SAN</option>
  </select>

  <label>Flight No.:</label>
  <input type="text" name="flightNo">

  <label>Departure:</label>
  <input type="datetime-local" name="departs" value="<%= departsDate %>">
  
  <input class="add-btn" type="submit" value="Add Flight">
  </div>

  
</form>

<%- include('../partials/footer') %>